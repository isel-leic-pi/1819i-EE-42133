<div>
    <h4>Teams:</h4>
</div>
<div class="container mt-3">
    <div class="row">
        <div class="card-deck">
            {{#each this}}
            <div class="card mb-4" style="min-width: 18rem; max-width: 18rem;" id="group_team_{{id}}">
                <div class="card-body">
                    <button type="button" class="close" aria-label="Close" id="close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <h5 class="card-title">{{name}}</h5>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</div>